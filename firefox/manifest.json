{
  "name": "WebVR Link Traversal",
  "short_name": "WebVR Links",
  "description": "A browser extension that allows you to seamlessly navigate between WebVR pages without taking off a headset.",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/commands",
  "manifest_version": 2,
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "applications": {
    "gecko": {
      "id": "webvr-link-traversal@mozilla.org",
      "strict_min_version": "48.0a1"
    }
  },
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      },
      "description": "Send a 'toggle-feature' event to the extension"
    }
  },
  "browser_action": {
    "default_icon": {
      "38": "img/icons/icon_38.png"
    }
  }
}
